{{#authMethods}}
{{#isOAuth}}
    {{#isCode}}
spring.security.oauth2.client.registration.{{{name}}}.authorization-grant-type=authorization_code
spring.security.oauth2.client.provider.{{{name}}}.token-uri={{{tokenUrl}}}
spring.security.oauth2.client.provider.{{{name}}}.authorization-uri={{{authorizationUrl}}}
    {{/isCode}}
    {{#isPassword}}
spring.security.oauth2.client.registration.{{{name}}}.authorization-grant-type=password
spring.security.oauth2.client.provider.{{{name}}}.token-uri={{{tokenUrl}}}
    {{/isPassword}}
    {{#isApplication}}
spring.security.oauth2.client.registration.{{{name}}}.authorization-grant-type=client_credentials
spring.security.oauth2.client.provider.{{{name}}}.token-uri={{{tokenUrl}}}
    {{/isApplication}}
    {{#isImplicit}}
spring.security.oauth2.client.registration.{{{name}}}.authorization-grant-type=implicit
spring.security.oauth2.client.provider.{{{name}}}.authorization-uri={{{authorizationUrl}}}
    {{/isImplicit}}
{{/isOAuth}}
